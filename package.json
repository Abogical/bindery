{
  "name": "bindery",
  "version": "2.0.0-alpha.9.4",
  "description": "Book layout in the browser",
  "main": "dist/bindery.cjs.js",
  "module": "dist/bindery.esm.js",
  "browser": "dist/bindery.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/evnbr/bindery.git"
  },
  "dependencies": {
    "hyperscript": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.13",
    "postcss-inline-svg": "^3.0.0",
    "postcss-svg": "^2.2.0",
    "rollup": "^0.46.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-uglify": "^2.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^2.1.1",
    "svgo-loader": "^1.2.1",
    "webpack": "^3.4.1",
    "webpack-auto-inject-version": "^0.5.14"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "stats": "webpack --json > stats.json",
    "moduleSize": "webpack --config webpack.config.js -p --display-modules",
    "build2": "webpack --config webpack.config.js && cp dist/bindery.js docs/js && cp dist/bindery.js.map docs/js",
    "buildumd": "webpack --config webpack.umd.config.js",
    "minify": "webpack --config webpack.config.js -p",
    "dist": "webpack --config webpack.config.js && webpack --config webpack.config.js -p && webpack --config webpack.umd.config.js && webpack --config webpack.umd.config.js -p && cp dist/bindery.js docs/js",
    "docs": "cd docs && jekyll serve",
    "lint": "eslint src/ webpack.*.js --cache"
  },
  "keywords": [
    "book",
    "pagination",
    "print"
  ],
  "author": {
    "name": "Evan Brooks",
    "url": "evanbrooks.info"
  },
  "license": "MIT"
}
