{
  "name": "bindery",
  "version": "2.2.0",
  "description": "Book layout in the browser",
  "main": "dist/bindery.cjs.js",
  "module": "dist/bindery.esm.js",
  "browser": "dist/bindery.umd.js",
  "unpkg": "dist/bindery.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/evnbr/bindery.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^20.0.4",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-class-prefix": "^0.3.0",
    "rollup": "^0.46.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-babel-minify": "^4.0.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^0.5.6",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-sizes": "^0.4.2"
  },
  "scripts": {
    "build": "rollup -c && cp dist/bindery.umd.js docs/js && cp dist/bindery.umd.js.map docs/js",
    "dev": "rollup -c -w",
    "test": "jest --nocache && codecov",
    "coverage": "jest --coverage",
    "docs": "cd docs && jekyll serve",
    "lint": "eslint src/ webpack.*.js --cache"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "keywords": [
    "book",
    "pagination",
    "print"
  ],
  "author": {
    "name": "Evan Brooks",
    "url": "https://evanbrooks.info"
  },
  "license": "MIT"
}
